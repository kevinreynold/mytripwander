{
  "name": "framework7-vue-webpack-template",
  "description": "Framework7 Vue + Webpack template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build/dev-server.js",
    "distclean": "del /q dist\\* && for /d %p in (dist\\*.*) do rmdir \"%p\" /s /q",
    "build": "cross-env NODE_ENV=development node build/build.js",
    "pgclean": "del /q pg\\www\\* && for /d %p in (pg\\www\\*.*) do rmdir \"%p\" /s /q",
    "pgcopy": "xcopy res pg\\www\\res /e /i /h /k && xcopy dist\\static\\fonts dist\\static\\css\\static\\fonts /e /i /h /k && xcopy dist pg\\www /e /i /h /k",
    "pgrun": "cd pg && phonegap build android",
    "pg": "npm run distclean && npm run build && npm run pgclean && npm run pgcopy && npm run pgrun"
  },
  "author": "Reynold Kevin",
  "dependencies": {
    "framework7": "^1.6.5",
    "framework7-vue": "^0.9.2",
    "vue": "^2.2.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.26.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "framework7-icons": "^0.8.9",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "jquery": "^3.2.1",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.1.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^11.1.4",
    "vue-resource": "^1.3.4",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1",
    "travelpayouts-api": "1.0.0",
    "got": "^6.2.0",
    "ion-rangeslider": "2.2.0",
    "vue2-google-maps": "0.8.11",
    "vue-form-wizard" : "0.8.4"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
